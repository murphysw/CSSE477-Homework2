import javax.swing.JFrame;
import javax.swing.JLabel;


public class PluginManagerUI extends JFrame{
	
	private ExecutionPanel executionFrame;
	private ListingPanel listingFrame;
	private StatusPanel statusFrame;
	
	public PluginManagerUI() {
		super();
		setupUI();
	}
	
	public void setupUI(){
		executionFrame = new ExecutionFrame();
		listingFrame = new ListingFrame();
		statusFrame = new StatusFrame();
	}

}
